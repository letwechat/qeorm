        <sql>
            <![CDATA[ select * from `tag` where id={id} and name like '%{name}%' ]]>
        </sql>
    </sqlConfig>
    <sqlConfig id="findTagById"  >
        <sql>
            <![CDATA[ select * from `tag` where id={id} ]]>
        </sql>
    </sqlConfig>
    <sqlConfig id="count" >
